 {\r\n      if (this.board.content)\r\n        return this.board.content.split(\"\\n\").join(\"<br>\");\r\n      return \"\";\r\n    },\r\n    // changeDateFormat() {\r\n    //   return moment(new Date(this.article.regtime)).format(\r\n    //     \"YYYY.MM.DD hh:mm:ss\"\r\n    //   );\r\n    // },\r\n  },\r\n  created() {\r\n    getNotice(\r\n      this.$route.params.boardno,\r\n      (response) => {\r\n        this.board = response.data;\r\n      },\r\n      (error) => {\r\n        console.log(\"삭제시 에러발생!!\", error);\r\n      }\r\n    );\r\n  },\r\n  methods: {\r\n    listNotice() {\r\n      this.$router.push({ name: \"NoticeList\" });\r\n    },\r\n    moveModifyNotice() {\r\n      this.$router.replace({\r\n        name: \"NoticeUpdate\",\r\n        params: { boardno: this.board.boardno },\r\n      });\r\n      //   this.$router.push({ path: `/board/modify/${this.board.boardno}` });\r\n    },\r\n    removeNotice() {\r\n      if (confirm(\"정말로 삭제?\")) {\r\n        deleteNotice(this.board.boardno, () => {\r\n          this.$router.push({ name: \"NoticeList\" });\r\n        });\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"],"sourceRoot":"src/components/notice"}]}