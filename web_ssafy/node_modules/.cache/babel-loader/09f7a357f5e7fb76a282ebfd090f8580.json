ZA,KAAA;AAcA,GApBA;AAqBA,EAAA,QAAA,kCACA,QAAA,CAAA,WAAA,EAAA,CAAA,UAAA,CAAA,CADA;AAEA,IAAA,IAFA,kBAEA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,UAAA,KAAA,OAAA,CAAA,MAAA;AACA,aAAA,KAAA,OAAA,CAAA,MAAA;AACA;AALA,IArBA;AA4BA,EAAA,OA5BA,qBA4BA;AAAA;;AACA,QAAA,KAAA,GAAA;AACA,MAAA,EAAA,EAAA,KAAA,WADA;AAEA,MAAA,GAAA,EAAA,KAAA,OAFA;AAGA;AACA;AACA,MAAA,GAAA,EAAA,KAAA,GALA;AAMA,MAAA,IAAA,EAAA,KAAA;AANA,KAAA;AAQA,IAAA,UAAA,CACA,KADA,EAEA,UAAA,QAAA,EAAA;AACA,MAAA,KAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,KALA,EAMA,UAAA,KAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,KARA,CAAA;;AAUA,QAAA,KAAA,QAAA,CAAA,MAAA,IAAA,OAAA,EAAA;AACA,WAAA,OAAA,GAAA,IAAA;AACA;AACA,GAlDA;AAmDA,EAAA,OAAA,EAAA;AACA,IAAA,SADA,uBACA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KAHA;AAIA;AACA,IAAA,YALA,0BAKA;AAAA;;AACA,MAAA,IAAA,CACA,GADA,gDAEA,KAAA,GAFA,mBAEA,KAAA,IAFA,GAIA,IAJA,CAIA,gBAAA;AAAA,YAAA,IAAA,QAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,YAAA,KAAA,GAAA;AACA,UAAA,EAAA,EAAA,MAAA,CAAA,EADA;AAEA,UAAA,GAAA,EAAA,MAAA,CAAA,GAFA;AAGA;AACA;AACA,UAAA,GAAA,EAAA,MAAA,CAAA,GALA;AAMA,UAAA,IAAA,EAAA,MAAA,CAAA;AANA,SAAA;AAQA,QAAA,UAAA,CACA,KADA,EAEA,UAAA,QAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA;AACA,SAJA,EAKA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,SAPA,CAAA;AASA,OAvBA;AAwBA;AA9BA;AAnDA,CAAA","sourcesContent":["<template>\r\n  <b-container class=\"bv-example-row mt-3\">\r\n    <b-row>\r\n      <b-col>\r\n        <b-alert show><h3>글목록</h3></b-alert>\r\n      </b-col>\r\n    </b-row>\r\n    <b-row class=\"mb-1\">\r\n      <b-col class=\"text-right\">\r\n        <b-form-select\r\n          class=\"mr-2\"\r\n          v-model=\"key\"\r\n          size=\"ms\"\r\n          id=\"select\"\r\n          :options=\"options\"\r\n        ></b-form-select>\r\n        <input\r\n          type=\"text\"\r\n          id=\"word\"\r\n          @keyup.enter=\"searchNotice()\"\r\n          class=\"mr-2\"\r\n          v-model=\"word\"\r\n        />\r\n        <b-button variant=\"outline-info\" @click=\"searchNotice()\" class=\"mr-2\"\r\n          >검색\r\n        </b-button>\r\n        <b-button variant=\"outline-primary\" @click=\"moveWrite()\" v-if=\"isAdmin\"\r\n          >글쓰기</b-button\r\n        >\r\n      </b-col>\r\n    </b-row>\r\n    <b-row>\r\n      <b-col v-if=\"notices.length\">\r\n        <b-table-simple hover responsive>\r\n          <b-thead head-variant=\"dark\">\r\n            <b-tr>\r\n              <b-th>글번호</b-th>\r\n              <b-th>제목</b-th>\r\n              <b-th>조회수</b-th>\r\n              <b-th>작성자</b-th>\r\n              <b-th>작성일</b-th>\r\n            </b-tr>\r\n          </b-thead>\r\n          <tbody>\r\n            <!-- 하위 component인 ListRow에 데이터 전달(props) -->\r\n            <notice-list-row\r\n              v-for=\"(notice, index) in notices\"\r\n              :key=\"index\"\r\n              v-bind=\"notice\"\r\n              :per-page=\"perPage\"\r\n              :current-page=\"currentPage\"\r\n            />\r\n          </tbody>\r\n        </b-table-simple>\r\n      </b-col>\r\n      <b-col v-else class=\"text-center\">공지사항이 없습니다.</b-col>\r\n    </b-row>\r\n    <!-- Pagination 처리 -->\r\n    <b-pagination\r\n      v-model=\"currentPage\"\r\n      :total-rows=\"rows\"\r\n      :per-page=\"perPage\"\r\n      aria-controls=\"notice-list\"\r\n      align=\"center\"\r\n    ></b-pagination>\r\n  </b-container>\r\n</template>\r\n\r\n<script>\r\nimport NoticeListRow from \"@/components/notice/child/NoticeListRow\";\r\nimport { listNotice } from \"@/api/notice.js\";\r\n//import axios from \"axios\";\r\nimport http from \"@/util/http-common\";\r\nimport { mapState } from \"vuex\";\r\n\r\nconst memberStore = \"memberStore\";\r\n\r\nexport default {\r\n  name: \"NoticeList\",\r\n  components: {\r\n    NoticeListRow,\r\n  },\r\n  data() {\r\n    return {\r\n      // 검색\r\n      key: null,\r\n      options: [\r\n        { value: null, text: \"선택\" },\r\n        { value: \"subject\", text: \"제목\" },\r\n        { value: \"content\", text: \"내용\" },\r\n      ],\r\n      word: \"\",\r\n      notices: [],\r\n      isAdmin: false,\r\n      perPage: 3,\r\n      currentPage: 1,\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState(memberStore, [\"userInfo\"]),\r\n    rows() {\r\n      console.log(\"길이 : \" + this.notices.length);\r\n      return this.notices.length;\r\n    },\r\n  },\r\n  created() {\r\n    let param = {\r\n      pg: this.currentPage,\r\n      spp: this.perPage,\r\n      // pg: 1,\r\n      // spp: 2,\r\n      key: this.key,\r\n      word: this.word,\r\n    };\r\n    listNotice(\r\n      param,\r\n      (response) => {\r\n        this.notices = response.data;\r\n        console.log(response.data);\r\n      },\r\n      (error) => {\r\n        console.log(error);\r\n      }\r\n    );\r\n    if (this.userInfo.userid == \"admin\") {\r\n      this.isAdmin = true;\r\n    }\r\n  },\r\n  methods: {\r\n    moveWrite() {\r\n      this.$router.push({ name: \"NoticeWrite\" });\r\n    },\r\n    // 검색\r\n    searchNotice() {\r\n      http\r\n        .get(\r\n          `http://localhost:9999/vue/notice?key=${this.key}&word=${this.word}`\r\n        )\r\n        .then(({ data }) => {\r\n          console.log(data);\r\n          let param = {\r\n            pg: this.pg,\r\n            spp: this.spp,\r\n            // pg: 1,\r\n            // spp: 10,\r\n            key: this.key,\r\n            word: this.word,\r\n          };\r\n          listNotice(\r\n            param,\r\n            (response) => {\r\n              this.notices = response.data;\r\n            },\r\n            (error) => {\r\n              console.log(error);\r\n            }\r\n          );\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scope>\r\n.tdClass {\r\n  width: 50px;\r\n  text-align: center;\r\n}\r\n.tdSubject {\r\n  width: 300px;\r\n  text-align: left;\r\n}\r\n#select {\r\n  width: 100px;\r\n}\r\ninput {\r\n  vertical-align: middle;\r\n  height: 35px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/notice"}]}