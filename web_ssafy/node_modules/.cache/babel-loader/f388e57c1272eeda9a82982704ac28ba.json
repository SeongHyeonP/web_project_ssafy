A,OAnBA,CAFA;AAuBA,MAAA,SAAA,EAAA,CAvBA;AAwBA,MAAA,WAAA,EAAA,CAxBA;AAyBA,MAAA,OAAA,EAAA,CAzBA;AA0BA,MAAA,WAAA,EAAA,CAAA,CAAA,EAAA,CAAA,EAAA,EAAA,CA1BA;AA2BA,MAAA,MAAA,EAAA,EA3BA;AA4BA,MAAA,QAAA,EAAA,KA5BA;AA6BA,MAAA,aAAA,EAAA,KA7BA;AA8BA,MAAA,SAAA,EAAA;AACA,QAAA,EAAA,EAAA,YADA;AAEA,QAAA,KAAA,EAAA,EAFA;AAGA,QAAA,OAAA,EAAA;AAHA;AA9BA,KAAA;AAoCA,GA3CA;AA4CA,EAAA,QAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA;AACA,aAAA,KAAA,MAAA,CACA,MADA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,QAAA;AAAA,OADA,EAEA,GAFA,CAEA,UAAA,CAAA,EAAA;AACA,eAAA;AAAA,UAAA,IAAA,EAAA,CAAA,CAAA,KAAA;AAAA,UAAA,KAAA,EAAA,CAAA,CAAA;AAAA,SAAA;AACA,OAJA,CAAA;AAKA;AARA,GA5CA;AAsDA,EAAA,OAtDA,qBAsDA;AAAA;;AACA,IAAA,IAAA,CACA,IADA,mBACA;AACA,MAAA,QAAA,EAAA,KAAA,QADA;AAEA,MAAA,SAAA,EAAA,KAAA;AAFA,KADA,EAKA,IALA,CAKA,gBAAA;AAAA,UAAA,IAAA,QAAA,IAAA;;AACA,UAAA,IAAA,IAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,SAAA;AACA;AACA,KAVA;AAWA,SAAA,SAAA,GAAA,KAAA,SAAA,CAAA,MAAA;AACA;AAnEA,CAAA","sourcesContent":["<template>\r\n  <b-container fluid>\r\n    <!-- User Interface controls -->\r\n    <b-row>\r\n      <b-col sm=\"5\" md=\"6\" class=\"my-1\">\r\n        <b-form-group\r\n          label=\"한번에 보이는 줄 수\"\r\n          label-for=\"per-page-select\"\r\n          label-cols-sm=\"6\"\r\n          label-cols-md=\"4\"\r\n          label-cols-lg=\"3\"\r\n          label-align-sm=\"right\"\r\n          label-size=\"sm\"\r\n          class=\"mb-0\"\r\n        >\r\n          <b-form-select\r\n            id=\"per-page-select\"\r\n            v-model=\"perPage\"\r\n            :options=\"pageOptions\"\r\n            size=\"sm\"\r\n          ></b-form-select>\r\n        </b-form-group>\r\n      </b-col>\r\n\r\n      <b-col sm=\"7\" md=\"6\" class=\"my-1\">\r\n        <b-pagination\r\n          v-model=\"currentPage\"\r\n          :total-rows=\"totalRows\"\r\n          :per-page=\"perPage\"\r\n          align=\"fill\"\r\n          size=\"sm\"\r\n          class=\"my-0\"\r\n        ></b-pagination>\r\n      </b-col>\r\n    </b-row>\r\n\r\n    <b-table\r\n      :items=\"hospitals\"\r\n      :fields=\"fields\"\r\n      :current-page=\"currentPage\"\r\n      :per-page=\"perPage\"\r\n      :sort-by.sync=\"sortBy\"\r\n      :sort-desc.sync=\"sortDesc\"\r\n      :sort-direction=\"sortDirection\"\r\n      stacked=\"md\"\r\n      small\r\n    >\r\n      <template #cell(name)=\"row\">\r\n        {{ row.value.first }} {{ row.value.last }}\r\n      </template>\r\n\r\n      <template #cell(actions)=\"row\">\r\n        <b-button size=\"sm\" @click=\"row.toggleDetails\">\r\n          {{ row.detailsShowing ? \"Hide\" : \"Show\" }} Details\r\n        </b-button>\r\n      </template>\r\n\r\n      <template #row-details=\"row\">\r\n        <b-card class=\"text-left\">\r\n          <ul>\r\n            <li v-for=\"(value, key) in row.item\" :key=\"key\">\r\n              {{ key }}: {{ value }}\r\n            </li>\r\n          </ul>\r\n        </b-card>\r\n      </template>\r\n    </b-table>\r\n  </b-container>\r\n</template>\r\n\r\n<script>\r\nimport http from \"@/util/http-common\";\r\n\r\nexport default {\r\n  name: \"HospitalList\",\r\n  props: {\r\n    sidoName: String,\r\n    gugunName: String,\r\n  },\r\n  data() {\r\n    return {\r\n      hospitals: [],\r\n      fields: [\r\n        {\r\n          key: \"hospitalName\",\r\n          label: \"진료소명\",\r\n          sortable: true,\r\n          sortDirection: \"desc\",\r\n        },\r\n        {\r\n          key: \"address\",\r\n          label: \"주소\",\r\n          sortable: false,\r\n          class: \"text-center\",\r\n        },\r\n        {\r\n          key: \"phonenumber\",\r\n          label: \"연락처\",\r\n          sortable: false,\r\n          class: \"text-center\",\r\n        },\r\n        { key: \"actions\", label: \"상세정보\" },\r\n      ],\r\n      totalRows: 1,\r\n      currentPage: 1,\r\n      perPage: 3,\r\n      pageOptions: [3, 5, 10],\r\n      sortBy: \"\",\r\n      sortDesc: false,\r\n      sortDirection: \"asc\",\r\n      infoModal: {\r\n        id: \"info-modal\",\r\n        title: \"\",\r\n        content: \"\",\r\n      },\r\n    };\r\n  },\r\n  computed: {\r\n    sortOptions() {\r\n      // Create an options list from our fields\r\n      return this.fields\r\n        .filter((f) => f.sortable)\r\n        .map((f) => {\r\n          return { text: f.label, value: f.key };\r\n        });\r\n    },\r\n  },\r\n  created() {\r\n    http\r\n      .post(`/hospital/list`, {\r\n        sidoName: this.sidoName,\r\n        gugunName: this.gugunName,\r\n      })\r\n      .then(({ data }) => {\r\n        if (data != null) {\r\n          this.hospitals = data;\r\n          console.log(this.hospitals);\r\n        }\r\n      });\r\n    this.totalRows = this.hospitals.length;\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components/hospital"}]}