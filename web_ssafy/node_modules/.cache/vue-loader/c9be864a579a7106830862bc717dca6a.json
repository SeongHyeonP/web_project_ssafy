uter.push({ path: `/notice/modify/${this.notice.noticeno}` });\r\n    },\r\n    removeNotice() {\r\n      if (confirm(\"정말로 삭제?\")) {\r\n        deleteNotice(this.notice.noticeno, () => {\r\n          this.$router.push({ name: \"NoticeList\" });\r\n        });\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"]}]}